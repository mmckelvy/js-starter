{
  "name": "js-starter",
  "version": "1.0.0",
  "description": "Initial setup for a modern JS app",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "start": "node build/server.js",
    "build": "npm run build:server && npm run build:client",
    "build:prod": "NODE_ENV=production && npm run build",
    "build:server": "webpack --config config/webpack/server.js",
    "build:client": "webpack --config config/webpack/client.js",
    "watch:server": "webpack --config config/webpack/server.js --watch",
    "watch:client": "webpack --config config/webpack/client.js --watch"
  },
  "author": "Mark McKelvy",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.1.2",
    "file-loader": "^0.9.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "basic-static": "^2.0.1",
    "http-hash": "^2.0.0",
    "parse-body": "^1.0.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
